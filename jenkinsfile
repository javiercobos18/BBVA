pipeline {
  agent any
 parameters {
        string(name: 'name_branch', defaultValue: 'main', description: 'nombre del docker')
    }
    environment {
        name_final = "${name_container}${tag_imagen}${puerto_imagen}"        
    }
    stages {
		stage('Build') {
			node {
				echo 'Pulling...' + env.BRANCH_NAME
				checkout scm

			}
		 }             
      }   
  }
